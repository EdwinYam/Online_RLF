# RLF Prediction Configuration
# Copy this file to .env and fill in your values

# Data paths (comma-separated for multiple files)
DATA_CSV_PATHS=/datastore/AI_RLF/training_data_highway.csv,/datastore/AI_RLF/training_data_subway.csv

# Output directory for results
OUTPUT_DIR=results/optuna

# Optuna search configuration
N_TRIALS=50
MIN_FAR=0.02

# Training configuration
BATCH_SIZE=64
MAX_EPOCHS=100
PATIENCE=10

# Random seed for reproducibility
RANDOM_SEED=42

# Time configuration (15625 frc = 1 second)
FRC_PER_SECOND=15625
DELAY_SECONDS=0.9

# KNN configuration
KNN_MAX_SAMPLES=500
KNN_K_NEIGHBORS=5
KNN_RLF_CLASS_WEIGHT=3.0
